{
  "name": "tsdown-starter",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "A simple example library built by tsdown",
  "exports": {
    "./main": "./dist/main.mjs",
    "./schema": "./dist/schema.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "bunx --bun tsdown --watch src",
    "lint": "bunx --bun eslint --fix src",
    "format": "prettier --write src"
  },
  "devDependencies": {
    "@tiny/eslint": "workspace:*",
    "bun-types": "^1.3.5",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "drizzle-orm": "^0.45.1"
  }
}
